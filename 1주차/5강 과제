from random import *
id = randint(1,20)
id = []
for i in range(1,21) : 
    id.append(i)
    
#id = range(1, 21) 
#id = list(id) 도 가능

shuffle(id) # 리스트를 뒤섞기 
select = sample(id,4) # 리스트 내에서 1개를 무작위로 뽑기
chicken = select[0]
coffee = select[1:4]
print(" -- 당첨자 발표 --")
print(f"치킨 당첨자 : {chicken}")
print(f"커피 당첨자 : {coffee}")
print("-- 축하합니다 --")

